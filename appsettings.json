{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "MySqlConnection": "Server=db;Port=3306;Database=clinagenda_database;Uid=${DB_USER};Pwd=${DB_PASSWORD};"
  },

  "Flyway": {
    "User": "%DB_USER%",
    "Password": "%DB_PASSWORD%",
    "Database": "clinagenda_database",
    "Url": "jdbc:mysql://db:3306/clinagenda_database"
  }
}
